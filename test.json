  "info": {
    "title": "Tio Patinhas API",
    "description": "Prices management",
    "version": "0.0.1",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "token": {
        "type": "apiKey",
        "name": "token",
        "in": "header"
      }
    },
    "schemas": {
      "CreateAmountDto": {
        "type": "object",
        "properties": {
          "currency": {
            "type": "string",
            "default": "BRL",
            "enum": [
              "BRL",
              "USD"
            ]
          },
          "amount": {
            "type": "number"
          },
          "precision": {
            "type": "object",
            "default": 2
          }
        },
        "required": [
          "currency",
          "amount",
          "precision"
        ]
      },
      "CreateRulesDto": {
        "type": "object",
        "properties": {
          "period": {
            "type": "string",
            "default": "DAYS",
            "enum": [
              "DAYS",
              "MONTHS"
            ]
          },
          "number": {
            "type": "object",
            "default": 3
          },
          "amount": {
            "$ref": "#/components/schemas/CreateAmountDto"
          },
          "discount": {
            "type": "object",
            "default": 0
          }
        },
        "required": [
          "period",
          "number",
          "amount",
          "discount"
        ]
      },
      "CreateLangDto": {
        "type": "object",
        "properties": {
          "pt_BR": {
            "type": "string"
          },
          "en_US": {
            "type": "string"
          }
        },
        "required": [
          "pt_BR",
          "en_US"
        ]
      },
      "CreateFeeAmountDto": {
        "type": "object",
        "properties": {
          "currency": {
            "type": "string",
            "default": "BRL",
            "enum": [
              "BRL",
              "USD"
            ]
          },
          "amount": {
            "type": "number"
          },
          "precision": {
            "type": "object",
            "default": 2
          }
        },
        "required": [
          "currency",
          "amount",
          "precision"
        ]
      },
      "CreateFeeDto": {
        "type": "object",
        "properties": {
          "name": {
            "$ref": "#/components/schemas/CreateLangDto"
          },
          "description": {
            "$ref": "#/components/schemas/CreateLangDto"
          },
          "amount": {
            "$ref": "#/components/schemas/CreateFeeAmountDto"
          }
        },
        "required": [
          "name",
          "description",
          "amount"
        ]
      },
      "CreatePriceDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "salesType": {
            "type": "string",
            "default": "SUBSCRIPTION",
            "enum": [
              "SHORT_STAY",
              "SUBSCRIPTION"
            ]
          },
          "feeType": {
            "type": "string",
            "default": "NET",
            "enum": [
              "NET",
              "GROSS",
              "COMISSION"
            ]
          },
          "category": {
            "type": "string",
            "default": "BASE",
            "enum": [
              "BASE"
            ]
          },
          "currency": {
            "type": "string",
            "default": "BRL",
            "enum": [
              "BRL",
              "USD"
            ]
          },
          "ruleType": {
            "type": "string",
            "default": "PER_NIGHT",
            "enum": [
              "PER_NIGHT",
              "PER_PERSON"
            ]
          },
          "rules": {
